<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="579.0" prefWidth="663.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject7.SecondaryController">
    <children>
      <AnchorPane prefHeight="562.0" prefWidth="623.0">
         <children>
              <Label layoutX="21.0" layoutY="37.0" prefHeight="63.0" prefWidth="250.0" text="Video Ttitle">
               <font>
                  <Font name="Baghdad Regular" size="23.0" />
               </font>
            </Label>
              <Button fx:id="secondaryButton" layoutX="49.0" layoutY="402.0" onAction="#addFiletoDB" text="Add File">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <TextField fx:id="TitleField" layoutX="21.0" layoutY="100.0" prefHeight="25.0" prefWidth="563.0" promptText="Please enter the name of your video here">
               <font>
                  <Font name="Baghdad Regular" size="13.0" />
               </font></TextField>
            <Label layoutX="237.0" layoutY="153.0" prefHeight="34.0" prefWidth="149.0" text="Desired File">
               <font>
                  <Font name="Baghdad Regular" size="27.0" />
               </font>
            </Label>
            <TextField fx:id="showVideoData" layoutX="14.0" layoutY="208.0" prefHeight="25.0" prefWidth="535.0" promptText="add file">
               <font>
                  <Font name="Baghdad Regular" size="13.0" />
               </font></TextField>
            <Button fx:id="getFIleExtension" layoutX="549.0" layoutY="209.0" mnemonicParsing="false" onAction="#selectVideoFile" text="..." textFill="#220bd4" />
            <ListView fx:id="myListView" layoutX="14.0" layoutY="252.0" prefHeight="140.0" prefWidth="563.0" />
            <Button layoutX="294.0" layoutY="473.0" mnemonicParsing="false" onAction="#clearDatabase" prefHeight="44.0" prefWidth="260.0" text="Clear Complete DataBase" textFill="#ff0404">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button layoutX="44.0" layoutY="474.0" mnemonicParsing="false" onAction="#gobacktoMainDisplay" prefHeight="25.0" prefWidth="107.0" text="Back ">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button layoutX="296.0" layoutY="402.0" mnemonicParsing="false" onAction="#clearAllVideoData" prefHeight="40.0" prefWidth="233.0" text="Clear Current Selection">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Label layoutX="36.0" layoutY="2.0" prefHeight="30.0" prefWidth="563.0" text="Please add a name for your video file before selecting the video file you want to be played" textAlignment="CENTER" textFill="#093be3">
               <font>
                  <Font name="Baghdad Regular" size="17.0" />
               </font>
            </Label>
            <Label layoutX="242.0" layoutY="22.0" prefHeight="30.0" prefWidth="140.0" text="want to be played!" textFill="#093be3">
               <font>
                  <Font name="Baghdad Regular" size="17.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
